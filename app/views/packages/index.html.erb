<div class="page-header">
  <h1>
    Packages
    <!-- <a href="/packages/new" class="btn btn-lg btn-success">New Package</a> -->
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
          <!-- <th>Notes</th> -->
        <th>Tracking number</th>
        <th>Tracking arrival date</th>
        <!-- <th>Order</th> -->
        <th>Date of receipt</th>
        <th>Actions</th>
      </tr>

      <% @packages.each do |package| %>
      <tr>
        <!-- <td><%= package.notes %></td> -->
        <td><%= package.tracking_number %></td>
        <td><%= package.tracking_arrival_date %></td>
        <!-- <td><%= package.order_id %></td> -->
        <td><%= package.date_of_receipt %></td>
        <td>
          <a href="/packages/<%= package.id %>" class="btn btn-primary"><i class="fa fa-search" aria-hidden="true"></i></a>
          <a href="/packages/<%= package.id %>/edit" class="btn btn-warning"><i class="fa fa-pencil-square-o" aria-hidden="true"></i></a>
          <a href="/delete_package/<%= package.id %>" class="btn btn-danger" rel="nofollow"><i class="fa fa-trash" aria-hidden="true"></i></a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
