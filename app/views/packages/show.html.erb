<div class="page-header">
  <h3>Package #<%= @package.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Notes</dt>
      <dd><%= @package.notes %></dd>

      <dt>Tracking number</dt>
      <dd><%= @package.tracking_number %></dd>

      <dt>Tracking arrival date</dt>
      <dd><%= @package.tracking_arrival_date %></dd>

      <dt>Order</dt>
      <dd><%= @package.order_id %></dd>

      <dt>Date of receipt</dt>
      <dd><%= @package.date_of_receipt %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/packages" class="btn btn-primary">
        Back
      </a>
      <a href="/packages/<%= @package.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_package/<%= @package.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
